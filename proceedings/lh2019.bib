@Article{DELPHICollaboration1997,
author="{DELPHI Collaboration}",
title="Measurement of event shape and inclusive distributions at {\$}{\{}sqrt s{\}} = 130{\$} and 136 GeV",
journal="Zeitschrift f{\"u}r Physik C Particles and Fields",
year="1997",
month="Jun",
day="01",
volume="73",
number="2",
pages="229--242",
abstract="Inclusive charged particle and event shape distributions are measured using 321 hadronic events collected with the DELPHI experiment at LEP at effective centre of mass energies of 130 to 136 GeV. These distributions are presented and compared to data at lower energies, in particular to the precise Z data. Fragmentation models describe the observed changes of the distributions well. The energy dependence of the means of the event shape variables can also be described using second order QCD plus power terms. A method independent of fragmentation model corrections is used to determine $\alpha$s from the energy dependence of the mean thrust and heavy jet mass. It is measured to be: {\$}{\$}{\textleftarrow}pha {\_}s(133 {\{}â¤ª GeV{\}})={\{}0.116{\}}pm {\{}0.007{\}}{\_}{\{}exp-0.004theo{\}}^{\{}+0.005{\}}{\$}{\$} from the high energy data.",
issn="1431-5858",
doi="10.1007/s002880050312",
url="https://doi.org/10.1007/s002880050312"
}


