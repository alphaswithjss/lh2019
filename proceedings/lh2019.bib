@Article{TheALEPHCollaboration2004,
author="{The ALEPH Collaboration}",
title="Studies of QCD at {\$}{\{}{\backslash}rm e{\}}^ + {\{}{\backslash}rm e{\}}^-{\$}centre-of-mass energies between 91 and 209 GeV",
journal="The European Physical Journal C - Particles and Fields",
year="2004",
month="Jul",
day="01",
volume="35",
number="4",
pages="457--486",
abstract="The hadronic final states observed with the ALEPH detector at LEP in {\$}{\{}{\backslash}rm e{\}}^ + {\{}{\backslash}rm e{\}}^-{\$}annihilation are analysed using 730 pb-1 of data collected between 91 and 209 GeV in the framework of QCD. In particular event-shape variables and inclusive charged particle spectra are measured. The energy evolution of quantities derived from these measurements is compared to analytic QCD predictions. The mean charged particle multiplicity, the charged particle momentum spectrum and its peak position are compared to predictions of the modified-leading-logarithmic approximation. The strong coupling constant {\$}{\backslash}alpha{\_}s{\$}is determined from a fit of the QCD prediction to distributions of six event-shape variables at eight centre-of-mass energies. A study of non-perturbative power law corrections is presented.",
issn="1434-6052",
doi="10.1140/epjc/s2004-01891-4",
url="https://doi.org/10.1140/epjc/s2004-01891-4"
}

